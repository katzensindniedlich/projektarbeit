---
import config from '@config'

const { description=config.description } = Astro.props
---

{ description &&
    <meta 
        name=description property=og:description 
        content={description}
    >
}
<meta 
    property=og:image 
    content={new URL('android-chrome-512x512.png', Astro.site)}
>
<meta 
    name=generator 
    content={Astro.generator}
>